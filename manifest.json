{
  "manifest_version": 2,
  "name": "RainLoop Extension",
  "version": "1.0",
  "description": "To add metrics to the rainloop UI client",
  "content_scripts": [
    {
      "matches": [
        "*://webmail.clebard.cloud/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "permissions": [
    "activeTab",
    "https://*/*"
  ],
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "web_accessible_resources": [
    "style.css"
  ]
}