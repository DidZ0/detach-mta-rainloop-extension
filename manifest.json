{
  "manifest_version": 2,
  "name": "RainLoop Extension",
  "version": "1.0",
  "description": "Adds a button to RainLoop",
  "content_scripts": [
    {
      "matches": ["*://webmail.clebard.cloud/*"],
      "js": ["content.js"]
    }
  ],
  "permissions": [
    "activeTab",
    "https://*/*"
  ],
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "web_accessible_resources": [
    "style.css"
  ]
}
